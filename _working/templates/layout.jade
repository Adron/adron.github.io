    .header.header-over.large
      .container
        .row
          .col-md-3.col-sm-6.col-xs-6
            // Logo Image
            a.logo-image.logo-animated(href=locals.url + '/index.html')
              img(src=locals.url + '/img/logos/logo.png', alt='logo')
            // End of Logo Image
          .col-md-9.col-sm-6.col-xs-6
            // Menu
            nav.right.helper
              ul.menu.sf-menu.js-menu
                li
                  a(href='/thrashingcodenews.html') Thrashing Code News
                li
                  a(href='#') Conf/Meetup/UserGroup Events
                  ul
                    li
                      a(href='/docs/Speaking-Presentations-Workshops/') Speaking - Presentations, Workshops, Etc
                li
                  a(href='/docs/about/') About
            // End of Menu
    .header-back.header-back-simple.header-back-small
      .header-back-container
        .container
          .row
            .col-md-12
              // Page Info
              .page-info.page-info-simple
                block header
                  block prepend title
                  h1= locals.name
                  h2.description= locals.description
              // End Page Info
    #content
      .container
        .layout.with-right-sidebar.js-layout
          .row
            .col-md-9
              .main-content
                // Blog Page
                block content
                // End of Blog Page
            .col-md-3.hidden-sm.hidden-xs
              .sidebar.js-sidebar-fixed
                // Vertical Menu
                .widget
                  // Search
                  .search
                    form(action="https://google.com/search", method="get")
                      input(type="hidden", name="sitesearch" value="http://blog.adron.me")
                      input.search-input.form-control(type="text", name="q", placeholder="search")
                      i.fa.fa-search.search-icon
                  // End of Search
                nav.menu-vertical-wrapper
                  ul.menu-vertical.js-menu-vertical(data-prepend-to='.js-layout', data-select='Menu')
                    li
